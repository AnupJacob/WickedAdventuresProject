apiVersion: eksctl.io/v1alpha5
kind: ClusterConfig

metadata:
  name: wwa-app-eks-cluster
  region: us-east-1

nodeGroups:
  - name: ng-1
    instanceType: t2.micro
    desiredCapacity: 1
    volumeSize: 80
    ssh:
      publicKeyPath: snehalshirs1-key
      allow: true # will use ~/.ssh/id_rsa.pub as the default ssh key
  - name: ng-2
    instanceType: t2.micro
    desiredCapacity: 1
    volumeSize: 100
    ssh:
      publicKeyPath: snehalshirs1-key
